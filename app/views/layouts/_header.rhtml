<div id="header">
  <div id="usernav">
    <% if logged_in? %>
      <strong>You are logged in as <%= current_user.login %></strong>
      <%= link_to 'Logout', logout_path %>
    <% else %>
      <%= link_to 'Login', login_path %> |
      <%= link_to 'Sign Up', signup_path %>
    <% end %>
  </div>
</div>
